{% extends 'AdminBundle:Default:baseAdmin.html.twig' %}

    {% block title %}<title>Update {{ boursier.getFullName() }}</title>{% endblock %}

    {% block body %}
        
        <div id="page-wrapper">
            <div class="row" style="background-color:#B4DEC9;">
                <div class="col-lg-12">            
                    <h1 class="page-header">Update {{ boursier.getFullName() }}</h1>                                  
                </div>
            </div>
            
                              
                
            <div class="container" style="margin-top:45px;">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="form-group">
                        {{ tinymce_init() }}
                        {{form_start(form)}}                           
                           <p>{{form_row(form.name) }}</p>
                           <p>{{form_row(form.firstname) }}</p>
                           <p>{{form_row(form.adress) }}</p>
                           <p>{{form_row(form.postcode) }}</p>
                           <p>{{form_row(form.city) }}</p>
                           <p>{{form_row(form.telfix) }}</p>
                           <p>{{form_row(form.telpor) }}</p>
                           <p>{{form_row(form.telegy) }}</p>
                           <p>{{form_row(form.arrivalDate) }}
                              {{form_row(form.birthDate) }}</p>
                           <p>{{form_row(form.domain) }}</p>
                           <p>{{form_row(form.subject) }}</p>
                           <p>{{form_row(form.universityEgypt) }}</p>
                           <p>{{form_row(form.universityFrench) }}</p>
                           <p>{{form_row(form.comment) }}</p>
                            <div style="text-align:center;margin-top:35px;">
                               {{form_end(form)}}
                            </div>
                        </div> 
                    </div>
                </div>
            </div>
        </div>

	{% endblock %}

		              

           