<!DOCTYPE html>
<html>


    <head>

        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="description" content="">
        <meta name="author" content="">

        {% block title %}<title>Egyptian Bureau for Cultural and Educational Affairs</title>{% endblock %}        

        <!-- Bootstrap Core CSS -->
        <link href="{{ asset('bundles/framework/css/bootstrap.css') }}" rel="stylesheet">                           
        <link href="{{ asset('bundles/framework/css/business-casual.css') }}" rel="stylesheet">            
        <link href="{{ asset('bundles/framework/css/sb-admin-2.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/framework/css/sb-admin-2.min.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/framework/css/metisMenu.css') }}" rel="stylesheet">
        <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/dt/dt-1.10.12/datatables.min.css"/>            

        <!-- Bootstrap Fonts CSS -->            
        <link href="https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800" rel="stylesheet" type="text/css">
        <link href="https://fonts.googleapis.com/css?family=Josefin+Slab:100,300,400,600,700,100italic,300italic,400italic,600italic,700italic" rel="stylesheet" type="text/css">            

        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
            <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>


    <body style="background-color: #b1c9dd;">

        {% block navbar %}
            <div class="brand">Egyptian Bureau for Cultural and Educational Affairs</div>
            <div class="address-bar">
                <div class="address-bar" style="overflow:auto;">
                    <div class="col-md-4">
                        <a href="{{ path('article_homepage_arabe') }}"><img  src="{{ asset('bundles/framework/img/flag-eg.png') }}" style="height:35px;width:45px;" title="Arab language"></a>
                    </div>
                    <div class="col-md-4">EBCEA</div>
                    <div class="col-md-4">
                       <a href="{{ path('article_homepage') }}"><img  src="{{ asset('bundles/framework/img/flag-en.png') }}" style="height:35px;width:45px;" title="English language"/></a>
                    </div>
                </div>
            </div>
            <!-- Navigation -->
            <nav class="navbar navbar-default" role="navigation">
                <div class="container">
                    <!-- Brand and toggle get grouped for better mobile display -->
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <!-- navbar-brand is hidden on larger screens, but visible when the menu is collapsed -->
                        <a class="navbar-brand" href="{{ path('article_homepage') }}" style="margin-bottom:10px;">Egyptian Bureau for Cultural and Educational  Affairs</a>
                    </div>
                    <!-- Collect the nav links, forms, and other content for toggling -->
                    
                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                        <ul class="nav navbar-nav">
                            <li>
                                <a href="{{ path('article_homepage') }}">Home</a>
                            </li>                           
                            <!-- <li>
                                <a href="{{ path('article_books_list') }}">Library</a>
                            </li> -->
                            <li>
                                <a href="{{ path('article_school_presentation') }}">School</a>
                            </li>                                                    
                            <li>
                                <a href="{{ path('article_service_list') }}">Services</a>
                            </li>
                            <li>
                                <a href="{{ path('article_partner_listPartnerPublic') }}">Partnerships</a>
                            </li>                                                                                                            
                        </ul>                   

                        <ul class="nav navbar-nav">
                            {% if is_granted('ROLE_ADMIN') or is_granted('ROLE_EDITOR') or is_granted('ROLE_MANAGEMENT') %}
                                <li>
                                    <a href="{{ path('admin_homepage') }}">Dashboard</a>
                                </li>
                            {% endif %}                                                                             
                            <!-- <li>
                                <a href="{{ path('article_centre_culturel_accueil') }}">Cultural Center</a>
                            </li> -->
                            {% if is_granted('ROLE_USER') %}
                            <li>
                                <a href="{{ path('article_boursier_accueil') }}">Scholars homepage</a>
                            </li>                            
                                <li>
                                    <a href="{{ path('fos_user_security_logout') }}">Log out</a>
                                </li>
                            {% else %}
                                <li>
                                    <a href="{{ path('fos_user_security_login') }}">Log in</a>
                                </li>                                                           
                            {% endif %}
                        </ul>                                
                    </div>
                                        
                    <!-- /.navbar-collapse -->
                </div>
                <!-- /.container -->
            </nav>
        {% endblock %}

    {% block body %}   {% endblock %}

    {% block footer %}

       
            <div class="row">
                <div class="box">
                    <div class="col-lg-12 text-center">
                        <p>Copyright &copy; Egyptian Bureau for Cultural and Educational Affairs 2016</p><br>
                        <p> 4 Chesterfield Gardens, Mayfair, W1J 5BG London - Landline: +44 20 7491 7720</p>
                        <hr/>
                        <ul class="nav navbar-nav">
                            <li>
                                <a href="{{ path('article_about') }}">Who we are ?</a>
                            </li>
                            <li>
                                <a href="{{ path('article_contact') }}">Contact</a>
                            </li>
                        </ul>                             
                    </div>
                </div>
            </div>
       

    {% endblock %}


    {% block javascripts %}

            <!-- jQuery -->
        <script
              src="https://code.jquery.com/jquery-3.1.0.min.js"
              integrity="sha256-cCueBR6CsyA4/9szpPfrX3s49M9vUU5BgtiJj06wt/s="
              crossorigin="anonymous"></script>

        <!-- Bootstrap Core JavaScript -->
        <script src="{{ asset('bundles/framework/js/bootstrap.js') }}"></script>
        <script src="{{ asset('bundles/framework/js/flot-data.js') }}"></script>
        <script src="{{ asset('bundles/framework/js/morris-data.js') }}"></script>
        <script src="{{ asset('bundles/framework/js/sb-admin-2.js') }}"></script>
        <script src="{{ asset('bundles/framework/js/morris.js') }}"></script>
        <script src="{{ asset('bundles/framework/js/metisMenu.js') }}"></script>
        <script src="https://use.fontawesome.com/6c9093b244.js"></script>
        <script type="text/javascript" src="https://cdn.datatables.net/v/bs/dt-1.10.12/datatables.min.js"></script>


        <!-- Script to Activate the Carousel -->
        <script>
        $('.carousel').carousel({
            interval: 3000 //changes the speed
        })
        </script>

        <script>
            $(document).ready(function() {
                $('.js-datepicker').datepicker({
                    format: 'yyyy-mm-dd',
                    autoclose:true
                });
            });
        </script>
        <script type="text/javascript">
                    $(document).ready(function () {
                $('#dataTables-example').DataTable();
            });
        </script>

    {% endblock %}

    </body>
</html>
